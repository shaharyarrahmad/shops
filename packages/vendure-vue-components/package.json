{
  "name": "vendure-vue-components",
  "version": "0.60.6",
  "description": "Vue components for building ",
  "author": "Martijn van de Brug <martijn@pinelab.studio>",
  "homepage": "https://pinelab.studio",
  "repository": "https://github.com/martijnvdbrug/shops/tree/master/packages/",
  "license": "MIT",
  "private": false,
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "README.md"
  ],
  "scripts": {
    "build": "rm -rf lib && yarn tsc --build && yarn copy",
    "dev-build": "yarn tsc --build && yarn copy",
    "copy": "copyfiles -u 1 'src/components/**/*' lib",
    "generate": "graphql-codegen",
    "watch": "nodemon --verbose --ext js,ts,vue --watch ./src --exec 'yarn dev-build'",
    "test": "jest",
    "generate-docs": "vue-docgen -c vue-docgen.config.js && yarn concat-docs",
    "concat-docs": "concat-md --toc --tocLevel 2 --decrease-title-levels --file-name-as-title docs > README.md"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.3",
    "@graphql-codegen/typescript": "1.21.1",
    "@graphql-codegen/typescript-operations": "1.17.15",
    "@types/jest": "^27.4.0",
    "@types/node": "^15.12.2",
    "concat-md": "^0.5.0",
    "copyfiles": "^2.4.1",
    "gridsome": "^0.7.23",
    "jest": "^27.4.6",
    "nodemon": "^2.0.7",
    "ts-jest": "^27.1.2",
    "typescript": "^4.2.3",
    "vue-docgen-cli": "^4.56.1"
  },
  "dependencies": {
    "graphql": "15.3.0",
    "graphql-request": "^3.4.0",
    "mitt": "^3.0.0"
  }
}
