{
  "name": "shops",
  "version": "0.18.7",
  "private": true,
  "scripts": {
    "start": "node ./dist/index",
    "debug:test": "yarn build && export SHOP_ENV=.env.test && node --inspect ./dist/index-dev",
    "serve2": "nodemon -e ts  --ignore './src/__admin-ui/' --exec \"yarn ts-node ./src/index-dev.ts\"",
    "serve": "nodemon -e ts --watch '../packages/' --watch './src/' --exec \"yarn build && node ./dist/index-dev\"",
    "serve:prod": "export SHOP_ENV=.env && yarn serve",
    "serve:test": "export SHOP_ENV=.env.test && yarn serve",
    "copy": "yarn copyfiles -u 1 'src/__admin-ui/dist/**/*' dist",
    "build": "yarn tsc && yarn copy",
    "migration:generate": "yarn ts-node migration generate",
    "migration:run": "yarn ts-node migration run",
    "migration:revert": "yarn ts-node migration revert",
    "test": "export SHOP_ENV=.env.test && ",
    "prod": "export SHOP_ENV=.env && "
  },
  "resolutions": {
    "tslib": "1.11.2"
  },
  "dependencies": {
    "@mollie/api-client": "^3.2.4",
    "@types/localtunnel": "^2.0.0",
    "@types/tmp": "^0.2.0",
    "@vendure/admin-ui-plugin": "0.18.2",
    "@vendure/asset-server-plugin": "0.18.2",
    "@vendure/core": "0.18.2",
    "@vendure/email-plugin": "0.18.2",
    "@vendure/ui-devkit": "0.18.2",
    "copyfiles": "^2.4.1",
    "localtunnel": "^2.0.0",
    "mysql": "^2.18.1",
    "tmp": "^0.2.1",
    "typescript": "4.0.3",
    "vendure-plugin-google-storage-assets": "^0.18.6",
    "vendure-plugin-webhook": "^0.18.7",
    "vendure-plugin-public-stock": "^0.18.7"
  },
  "devDependencies": {
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0"
  }
}
